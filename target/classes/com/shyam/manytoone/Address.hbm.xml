<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.shyam.manytoone.Address" table="ADDRESS">
	   <id name="address_id" type="long" column="ADDRESS_ID"> 
        <generator class="identity" /> 
   		</id> 
        
	
	<property name="address_city" type="string" column="ADDRESS_CITY">
	    
	</property>
	
	<property name="address_state" type="string" column="ADDRESS_STATE">
	    
	</property>

	<property name="address_zipcode" type="string" column="ADDRESS_ZIPCODE">
	    
	</property>	
        
    </class>
</hibernate-mapping>